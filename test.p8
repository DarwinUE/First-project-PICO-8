pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
--player

p = {
	x=64,
	y=64,
	anim="talk",
	walk={f=1,st=1,sz=2,spd=1/5},
	talk={f=17,st=17,sz=3,spd=1/5}
}
 
ctime = {
	m = 0,
	s = 0,
	ms = 0
}	


function _update()
	
	controls()
	time_manager()
	
end 


function _draw()
	cls()
	map(0,0, 0,0, 16,16)
	highlight(anim_player(),p.x,p.y,7)
	print(ctime.s)
	highlight(6,120,4,2)
	
end


function controls()
	
	p.anim = "walk"
	if (btn(0)) p.x -= 1
	if (btn(1)) p.x += 1
	if (btn(2)) p.y -= 1
	if (btn(3)) p.y += 1
	if ((btn(0) or btn(1) or
	    btn(2) or btn(3)) == false) then
	 p.anim = "talk"   
	end
end 


function time_manager ()
	
	ctime.ms += 1/30
	if (ctime.ms >= 1) then
		ctime.ms = 0 
		ctime.s += 1
		if (ctime.s >= 60) then
			ctime.s = 0 
			ctime.m += 1
			end
		end
end 


function anim(a)

	a.f += a.spd
	if (a.f >= a.st + a.sz) then
		a.f = a.st
	end
	return flr(a.f)

end


function anim_player()

	if (p.anim == "talk") then
		return anim(p.talk)
	else
		return anim(p.walk)
	end
end


function highlight(f,x,y,col)

	for i=1, 16 do	
		pal(i,col)	
	end
	spr(f,x,y+1)
	spr(f,x,y-1)
	spr(f,x+1,y)
	spr(f,x-1,y)
	pal()
	spr(f,x,y)
	

end
